{
  "name": "sparkusb-client",
  "author": "REV Robotics",
  "description": "Client for SPARK MAX",
  "version": "0.6.4",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "@blueprintjs/core": "^3.0.1",
    "@blueprintjs/select": "^3.0.0",
    "better-queue": "^3.8.10",
    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.13",
    "protobufjs": "^6.8.8",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "serialport": "^6.2.1",
    "zeromq": "^4.6.0"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "electron": "^2.0.5",
    "electron-builder": "^20.22.0",
    "electron-debug": "^2.0.0",
    "electron-rebuild": "^1.8.1",
    "gulp": "^3.9.1"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm run desktop\" \"npm run react\"",
    "desktop": "electron .",
    "react": "react-scripts start",
    "prebuild": "electron-rebuild",
    "build": "react-scripts build",
    "postbuild": "gulp post-build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "postinstall": "npm rebuild --runtime=electron --target=2.0.5 --disturl=https://atom.io/download/atom-shell",
    "dist": "build"
  },
  "build": {
    "appId": "org.revrobotics.tng.client.0.6.4",
    "productName": "SPARK MAX Client",
    "copyright": "2018 REV Robotics LLC",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraFiles": [
      {
        "from": "sparkusb/libzmq-v120-mt-4_0_4.dll",
        "to": ".",
        "filter": ["*.dll"]
      }
    ],
    "directories": {
      "output": "dist",
      "buildResources": "build/resources"
    },
    "win": {
      "target": "NSIS"
    }
  },
  "homepage": "./"
}
