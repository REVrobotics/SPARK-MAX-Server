{
  "name": "sparkusb-client",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "dependencies": {
    "grpc": "^1.13.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "electron": "^2.0.5",
    "electron-debug": "^2.0.0"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm run desktop\" \"npm run react\"",
